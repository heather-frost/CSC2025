						;; Call gameLoop() - No Parameters, no return value
 00000005  E8 00000000 E		call gameLoop
						;; Call ExitProcess(uExitCode)
 0000000A  6A 00		    push  0
 0000000C  E8 00000000 E	    call  _ExitProcess@4
 00000011			main ENDP
				END
